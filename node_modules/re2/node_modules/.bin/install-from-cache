#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../install-artifact-from-github/bin/install-from-cache.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../install-artifact-from-github/bin/install-from-cache.js" "$@"
  ret=$?
fi
exit $ret
